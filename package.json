{
  "name": "nogosolo",
  "version": "1.0.0",
  "description": "NoGoSolo",
  "main": "index.js",
  "engines": {
    "node": "^6.11.5"
  },
  "scripts": {
    "start": "node server/index.js",
    "react-dev": "webpack -d --watch",
    "server-dev": "nodemon server/index.js",
    "lint": "eslint . --ext .jsx,.js",
    "post": "pg_ctl -D /usr/local/var/postgres start",
    "initdb": "psql postgres -U nogosolo < database/schema.sql",
    "test-db": "mocha --bail --reporter nyan spec/dbtests.js; pkill -n node;"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nogosolo/nogosolo.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nogosolo/nogosolo/issues"
  },
  "homepage": "https://github.com/nogosolo/nogosolo#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "express": "^4.16.2",
    "jquery": "^3.2.1",
    "mocha": "^4.0.1",
    "pg-promise": "^7.3.1",
    "react": "^16.1.1",
    "react-click-outside": "^3.0.0",
    "react-dom": "^16.1.1",
    "react-router-dom": "^4.2.2",
    "request": "^2.83.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "webpack": "^3.8.1",
    "pre-commit": "^1.1.3",
    "tape": "^4.7.0"
  },
  "production": {
    "use_env_variable": "DATABASE_URL"
  }
}
